var mojokerto= ee.FeatureCollection("users/nurilassajdah/pertanian_mojokerto");
var mojokertokec= ee.FeatureCollection("users/nurilassajdah/kecamatan_mojokerto");
var sampel= ee.FeatureCollection("users/nurilassajdah/titik_sampel");

var img201307 = ee.Image('LANDSAT/LC08/C02/T1_TOA/LC08_118065_20130728').clip(mojokerto);
var img201310 = ee.Image('LANDSAT/LC08/C02/T1_TOA/LC08_118065_20131016').clip(mojokerto);
var img201409 = ee.Image('LANDSAT/LC08/C02/T1_TOA/LC08_118065_20140901').clip(mojokerto);
