//NDVI
var ndvi201307 = img201307.normalizedDifference(['B5', 'B4']).rename('ndvi1');
var ndvi201310 = img201310.normalizedDifference(['B5', 'B4']).rename('ndvi2');
var ndvi201409 = img201409.normalizedDifference(['B5', 'B4']).rename('ndvi3');
var ndvi201410 = img201410.normalizedDifference(['B5', 'B4']).rename('ndvi4');
var ndvi201508 = img201508.normalizedDifference(['B5', 'B4']).rename('ndvi5');
var ndvi201510 = img201510.normalizedDifference(['B5', 'B4']).rename('ndvi6');
var ndvi201608 = img201608.normalizedDifference(['B5', 'B4']).rename('ndvi7');
var ndvi201609 = img201609.normalizedDifference(['B5', 'B4']).rename('ndvi8');
var ndvi201708 = img201708.normalizedDifference(['B5', 'B4']).rename('ndvi9');
var ndvi201709 = img201709.normalizedDifference(['B5', 'B4']).rename('ndvi10');
var ndvi201808 = img201808.normalizedDifference(['B5', 'B4']).rename('ndvi11');
var ndvi201809 = img201809.normalizedDifference(['B5', 'B4']).rename('ndvi12');
var ndvi201908 = img201908.normalizedDifference(['B5', 'B4']).rename('ndvi13');
var ndvi201910 = img201910.normalizedDifference(['B5', 'B4']).rename('ndvi14');
var ndvi202008 = img202008.normalizedDifference(['B5', 'B4']).rename('ndvi15');
var ndvi202009 = img202009.normalizedDifference(['B5', 'B4']).rename('ndvi16');
var ndvi202108 = img202108.normalizedDifference(['B5', 'B4']).rename('ndvi17');
var ndvi202109 = img202109.normalizedDifference(['B5', 'B4']).rename('ndvi18');
var ndvi202208 = img202208.normalizedDifference(['B5', 'B4']).rename('ndvi19');
var ndvi202210 = img202210.normalizedDifference(['B5', 'B4']).rename('ndvi20');


//NDVI
{var minNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.min(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(minNDVI1, 'minNDVI Jul 2013');
var maxNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.max(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(maxNDVI1, 'maxNDVI Jul 2013')
var meanNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.mean(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(meanNDVI1, 'meanNDVI Jul 2013');
}

