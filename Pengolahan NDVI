var empty = ee.Image().byte();
var outline = empty.paint({
  featureCollection: mojokertokec,
  color: 1,
  width: 1});
Map.addLayer(outline, {palette: '000000'}, 'Batas Kecamatan');

Map.centerObject(mojokerto,10.3); 
