{var minNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.min(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(minNDVI1, 'minNDVI Jul 2013');
var maxNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.max(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(maxNDVI1, 'maxNDVI Jul 2013')
var meanNDVI1 = ee.Number(ndvi201307.reduceRegion({
reducer: ee.Reducer.mean(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(meanNDVI1, 'meanNDVI Jul 2013');
}

