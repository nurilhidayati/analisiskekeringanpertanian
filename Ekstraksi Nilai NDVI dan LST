//Nilai Minimum, Maksimum dan Rerata LST
{var minLST1 = ee.Number(LST201307.reduceRegion({
reducer: ee.Reducer.min(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(minLST1, 'minLST Jul 2013');
var maxLST1 = ee.Number(LST201307.reduceRegion({
reducer: ee.Reducer.max(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(maxLST1, 'maxLST Jul 2013')
var meanLST1 = ee.Number(LST201307.reduceRegion({
reducer: ee.Reducer.mean(),
