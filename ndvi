var soil = ee.Number(0.2)

//fvc
var fv1 =(ndvi201307.subtract(soil).divide(maxNDVI1.subtract(soil))).rename('fv1');
var fv2 =(ndvi201310.subtract(soil).divide(maxNDVI2.subtract(soil))).rename('fv2');
var fv3 =(ndvi201409.subtract(soil).divide(maxNDVI3.subtract(soil))).rename('fv3');
var fv4 =(ndvi201410.subtract(soil).divide(maxNDVI4.subtract(soil))).rename('fv4');
var fv5 =(ndvi201508.subtract(soil).divide(maxNDVI5.subtract(soil))).rename('fv5');
var fv6 =(ndvi201510.subtract(soil).divide(maxNDVI6.subtract(soil))).rename('fv6');
var fv7 =(ndvi201608.subtract(soil).divide(maxNDVI7.subtract(soil))).rename('fv7');
var fv8 =(ndvi201609.subtract(soil).divide(maxNDVI8.subtract(soil))).rename('fv8');
var fv9 =(ndvi201708.subtract(soil).divide(maxNDVI9.subtract(soil))).rename('fv9');
var fv10 =(ndvi201709.subtract(soil).divide(maxNDVI10.subtract(soil))).rename('fv10');
var fv11 =(ndvi201808.subtract(soil).divide(maxNDVI11.subtract(soil))).rename('fv11');
var fv12 =(ndvi201809.subtract(soil).divide(maxNDVI12.subtract(soil))).rename('fv12');
var fv13 =(ndvi201908.subtract(soil).divide(maxNDVI13.subtract(soil))).rename('fv13');
var fv14 =(ndvi201910.subtract(soil).divide(maxNDVI14.subtract(soil))).rename('fv14');
var fv15 =(ndvi202009.subtract(soil).divide(maxNDVI15.subtract(soil))).rename('fv15');
var fv16 =(ndvi202010.subtract(soil).divide(maxNDVI16.subtract(soil))).rename('fv16');
var fv17 =(ndvi202108.subtract(soil).divide(maxNDVI17.subtract(soil))).rename('fv17');
var fv18 =(ndvi202109.subtract(soil).divide(maxNDVI18.subtract(soil))).rename('fv18');
var fv19 =(ndvi202208.subtract(soil).divide(maxNDVI19.subtract(soil))).rename('fv19');
var fv20 =(ndvi202210.subtract(soil).divide(maxNDVI20.subtract(soil))).rename('fv20');

//termal 10
var termal10_1= img201307.select('B10');
var termal10_2= img201310.select('B10');
var termal10_3= img201409.select('B10');
var termal10_4= img201410.select('B10');
var termal10_5= img201508.select('B10');
var termal10_6= img201510.select('B10');
var termal10_7= img201608.select('B10');
var termal10_8= img201609.select('B10');
