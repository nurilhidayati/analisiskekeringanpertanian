var termal10_18 = img202109.select('B10');
var termal10_19 = img202208.select('B10');
var termal10_20 = img202210.select('B10');

//LSE10
var LSE10_1 = termal10_1.expression(
    '(0.971*(1-fvc))+(0.987*(fvc))',{
      'fvc': fv1.select('fv1'),}).rename('LSE10_1');
var LSE10_2 = termal10_2.expression(
    '(0.971*(1-fvc))+(0.987*(fvc))',{
      'fvc': fv2.select('fv2'),}).rename('LSE10_2');
var LSE10_3 = termal10_3.expression(
    '(0.971*(1-fvc))+(0.987*(fvc))',{
      'fvc': fv3.select('fv3'),}).rename('LSE10_3');
