var termal10_10 = img201709.select('B10');
var termal10_11 = img201808.select('B10');
var termal10_12 = img201809.select('B10');
var termal10_13 = img201908.select('B10');
var termal10_14 = img201910.select('B10');
var termal10_15 = img202009.select('B10');
var termal10_16 = img202010.select('B10');
