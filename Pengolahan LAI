//NDVI
var ndvi201307 = img201307.normalizedDifference(['B5', 'B4']).rename('ndvi1');
var ndvi201310 = img201310.normalizedDifference(['B5', 'B4']).rename('ndvi2');
var ndvi201409 = img201409.normalizedDifference(['B5', 'B4']).rename('ndvi3');
var ndvi201410 = img201410.normalizedDifference(['B5', 'B4']).rename('ndvi4');
