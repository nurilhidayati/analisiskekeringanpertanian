{var minLAI1 = ee.Number(LAI201307.reduceRegion({
reducer: ee.Reducer.min(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(minLAI1, 'minLAI Jul 2013');
var maxLAI1 = ee.Number(LAI201307.reduceRegion({
reducer: ee.Reducer.max(),
geometry: mojokerto,
scale: 30,
maxPixels: 1e9
}).values().get(0));
print(maxLAI1, 'maxLAI Jul 2013')
