    '((-4.7559 * ndvi)  + 41.233)', {
    'ndvi': ndvi201310.select('ndvi2')});
 
var LSTmax3 = ndvi201409.expression(
    '((-4.0345 * ndvi) + 32.297)', {
    'ndvi': ndvi201409.select('ndvi3')});

var LSTmax4 = ndvi201410.expression(
    '((-8.4437 * ndvi)  + 44.784)', {
    'ndvi': ndvi201410.select('ndvi4')});

var LSTmax5 = ndvi201508.expression(
    '((-6.8602 * ndvi) + 39.722)', {
    'ndvi': ndvi201508.select('ndvi5')});
 
var LSTmax6 = ndvi201510.expression(
    '((-1.9693 * ndvi) + 41.734)', {
    'ndvi': ndvi201510.select('ndvi6')});

var LSTmax7 = ndvi201608.expression(
    '((-1.9366 * ndvi) + 35.863)', {
    'ndvi': ndvi201608.select('ndvi7')});

var LSTmax8 = ndvi201609.expression(
    '((-3.8956 * ndvi) + 38.17)', {
    'ndvi': ndvi201609.select('ndvi8')});

var LSTmax9 = ndvi201708.expression(
    '((-5.2809* ndvi) + 38.455)', {
    'ndvi': ndvi201708.select('ndvi9')});

var LSTmax10 = ndvi201709.expression(
    '((-4.3123 * ndvi) + 42.421)', {
    'ndvi': ndvi201709.select('ndvi10')});

var LSTmax11 = ndvi201808.expression(
    '((-6.449 * ndvi) + 39.523)', {
    'ndvi': ndvi201808.select('ndvi11')});

var LSTmax12 = ndvi201809.expression(
    '((-10.29 * ndvi) + 47.252)', {
    'ndvi': ndvi201809.select('ndvi12')});

var LSTmax13 = ndvi201908.expression(
    '((-6.4009 * ndvi) + 39.085)', {
    'ndvi': ndvi201908.select('ndvi13')});

var LSTmax14 = ndvi201910.expression(
    '((-11.811 * ndvi) + 47.499)', {
    'ndvi': ndvi201910.select('ndvi14')});

var LSTmax15 = ndvi202008.expression(
    '((-2.013 * ndvi) + 35.669)', {
    'ndvi': ndvi202008.select('ndvi15')});

var LSTmax16 = ndvi202009.expression(
    '((-8.4381 * ndvi) + 43.624)', {
    'ndvi': ndvi202009.select('ndvi16')});
 
var LSTmax17 = ndvi202108.expression(
    '((-3.7928 * ndvi) + 32.617)', {
    'ndvi': ndvi202108.select('ndvi17')});

var LSTmax18 = ndvi202109.expression(
    '((-6.8959 * ndvi) + 41.178)', {
    'ndvi': ndvi202109.select('ndvi18')});

var LSTmax19 = ndvi202208.expression(
    '((-18.731 * ndvi) + 43.39)', {
    'ndvi': ndvi202208.select('ndvi19')});

var LSTmax20 = ndvi202210.expression(
    '((-9.5715 * ndvi) + 38.404)', {
    'ndvi': ndvi202210.select('ndvi20')});

//TVDI
var TVDI201307 =(LST201307.subtract(LSTmin1).divide(LSTmax1.subtract(LSTmin1)));
var TVDI201310 =(LST201310.subtract(LSTmin2).divide(LSTmax2.subtract(LSTmin2)));
var TVDI201409 =(LST201409.subtract(LSTmin3).divide(LSTmax3.subtract(LSTmin3)));
var TVDI201410 =(LST201410.subtract(LSTmin4).divide(LSTmax4.subtract(LSTmin4)));
var TVDI201508 =(LST201508.subtract(LSTmin5).divide(LSTmax5.subtract(LSTmin5)));
var TVDI201510 =(LST201510.subtract(LSTmin6).divide(LSTmax6.subtract(LSTmin6)));
var TVDI201608 =(LST201608.subtract(LSTmin7).divide(LSTmax7.subtract(LSTmin7)));
var TVDI201609 =(LST201609.subtract(LSTmin8).divide(LSTmax8.subtract(LSTmin8)));
var TVDI201708 =(LST201708.subtract(LSTmin9).divide(LSTmax9.subtract(LSTmin9)));
var TVDI201709 =(LST201709.subtract(LSTmin10).divide(LSTmax10.subtract(LSTmin10)));
var TVDI201808 =(LST201808.subtract(LSTmin11).divide(LSTmax11.subtract(LSTmin11)));
var TVDI201809 =(LST201809.subtract(LSTmin12).divide(LSTmax12.subtract(LSTmin12)));
var TVDI201908 =(LST201908.subtract(LSTmin13).divide(LSTmax13.subtract(LSTmin13)));
var TVDI201910 =(LST201910.subtract(LSTmin14).divide(LSTmax14.subtract(LSTmin14)));
var TVDI202008 =(LST202008.subtract(LSTmin15).divide(LSTmax15.subtract(LSTmin15)));
var TVDI202009 =(LST202009.subtract(LSTmin16).divide(LSTmax16.subtract(LSTmin16)));
var TVDI202108 =(LST202108.subtract(LSTmin17).divide(LSTmax17.subtract(LSTmin17)));
var TVDI202109 =(LST202109.subtract(LSTmin18).divide(LSTmax18.subtract(LSTmin18)));
var TVDI202208 =(LST202208.subtract(LSTmin19).divide(LSTmax19.subtract(LSTmin19)));
var TVDI202210 =(LST202210.subtract(LSTmin20).divide(LSTmax20.subtract(LSTmin20)));

var sld_intervals =
  '<RasterSymbolizer>' +
    '<ColorMap type="intervals" extended="false" >' +
      '<ColorMapEntry color="#ffffff" quantity="-3" label="NA"/>' +
      '<ColorMapEntry color="#7a8737" quantity="0.2" label="Basah (0 < TVDI ≤ 0.20)"/>' +
      '<ColorMapEntry color="#0ae042" quantity="0.4" label="Agak Basah (0.21 < TVDI ≤ 0.40)" />' +
      '<ColorMapEntry color="#fff70b" quantity="0.6" label="Normal (0.41 < TVDI ≤ 0.60)" />' +
      '<ColorMapEntry color="#ffaf38" quantity="0.8" label="Agak Kering (0.61 < TVDI ≤ 0.80)" />' +
      '<ColorMapEntry color="#ff641b" quantity="3" label="Kering (0.81 < TVDI ≤ 1)" />' +
    '</ColorMap>' +
  '</RasterSymbolizer>';

//Menampilkan hasil tvdi
Map.addLayer(TVDI201307.sldStyle(sld_intervals), {}, 'Jul 2013');
Map.addLayer(TVDI201310.sldStyle(sld_intervals), {}, 'Okt 2013');
Map.addLayer(TVDI201409.sldStyle(sld_intervals), {}, 'Sep 2014');
Map.addLayer(TVDI201410.sldStyle(sld_intervals), {}, 'Okt 2014');
Map.addLayer(TVDI201508.sldStyle(sld_intervals), {}, 'Agt 2015');
Map.addLayer(TVDI201510.sldStyle(sld_intervals), {}, 'Okt 2015'); 
Map.addLayer(TVDI201608.sldStyle(sld_intervals), {}, 'Agt 2016');
Map.addLayer(TVDI201609.sldStyle(sld_intervals), {}, 'Sep 2016');
Map.addLayer(TVDI201708.sldStyle(sld_intervals), {}, 'Agt 2017');
Map.addLayer(TVDI201709.sldStyle(sld_intervals), {}, 'Sep 2017');
Map.addLayer(TVDI201808.sldStyle(sld_intervals), {}, 'Agt 2018');
Map.addLayer(TVDI201809.sldStyle(sld_intervals), {}, 'Sep 2018');
Map.addLayer(TVDI201908.sldStyle(sld_intervals), {}, 'Agt 2019');
Map.addLayer(TVDI201910.sldStyle(sld_intervals), {}, 'Okt 2019');
Map.addLayer(TVDI202008.sldStyle(sld_intervals), {}, 'Agt 2020');
Map.addLayer(TVDI202009.sldStyle(sld_intervals), {}, 'Sep 2020');
Map.addLayer(TVDI202108.sldStyle(sld_intervals), {}, 'Agt 2021');
Map.addLayer(TVDI202109.sldStyle(sld_intervals), {}, 'Sep 2021');
Map.addLayer(TVDI202208.sldStyle(sld_intervals), {}, 'Agt 2022');
Map.addLayer(TVDI202210.sldStyle(sld_intervals), {}, 'Okt 2022');

// Pendeskripsian citra yang dimasukkan pada layer split panel
var images = {
      
      'TVDI Oktober 2022': TVDI202210.sldStyle(sld_intervals),
      'TVDI Agustus 2022': TVDI202208.sldStyle(sld_intervals),
