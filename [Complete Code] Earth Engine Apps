var fv17 =(ndvi202108.subtract(minNDVI1).divide(maxNDVI1.subtract(minNDVI1))).pow(ee.Number(2)).rename('fv17');
var fv18 =(ndvi202109.subtract(minNDVI1).divide(maxNDVI1.subtract(minNDVI1))).pow(ee.Number(2)).rename('fv18');
var fv19 =(ndvi202208.subtract(minNDVI1).divide(maxNDVI1.subtract(minNDVI1))).pow(ee.Number(2)).rename('fv19');
