      '<ColorMapEntry color="#ffaf38" quantity="0.8" label="0.6" />' +
      '<ColorMapEntry color="#ff641b" quantity="3" label="3" />' +
    '</ColorMap>' +
  '</RasterSymbolizer>';
// Memisahkan hasil dalam 8 kelas klasifikasi
var thresholds = ee.Image([-3, 0.2, 0.4, 0.6, 0.8, 3]);
var classified1 = TVDI201307.lt(thresholds).reduce('sum').toInt();
var classified2 = TVDI201310.lt(thresholds).reduce('sum').toInt();
var classified3 = TVDI201409.lt(thresholds).reduce('sum').toInt();
var classified4 = TVDI201410.lt(thresholds).reduce('sum').toInt();
