var classified6 = TVDI201510.lt(thresholds).reduce('sum').toInt();
var classified7 = TVDI201608.lt(thresholds).reduce('sum').toInt();
var classified8 = TVDI201609.lt(thresholds).reduce('sum').toInt();
var classified9 = TVDI201708.lt(thresholds).reduce('sum').toInt();
var classified10 = TVDI201709.lt(thresholds).reduce('sum').toInt();
var classified11 = TVDI201808.lt(thresholds).reduce('sum').toInt();
var classified12 = TVDI201809.lt(thresholds).reduce('sum').toInt();
var classified13 = TVDI201908.lt(thresholds).reduce('sum').toInt();
var classified14 = TVDI201910.lt(thresholds).reduce('sum').toInt();
var classified15 = TVDI202009.lt(thresholds).reduce('sum').toInt();
var classified16 = TVDI202010.lt(thresholds).reduce('sum').toInt();
var classified17 = TVDI202108.lt(thresholds).reduce('sum').toInt();
var classified18 = TVDI202109.lt(thresholds).reduce('sum').toInt();
var classified19 = TVDI202208.lt(thresholds).reduce('sum').toInt();
var classified20 = TVDI202210.lt(thresholds).reduce('sum').toInt();


//Menampilkan hasil tvdi
Map.addLayer(TVDI201307.sldStyle(sld_intervals), {}, 'Jul 2013');
