    } 
    else{
        tabelButton.setLabel('Tabel Luas Sebaran Kekeringan Pertanian');
        tabelPanel.style().set('shown', true);
    }
})
panel.add(tabelButton);
panel.add(tabelPanel);


var tabelLuas = ui.Panel(
    [
        ui.Label('Tabel Luas Sebaran Kekeringan Pertanian', {fontSize:'12px', fontWeight: 'bold', margin: '2px 15px 2px 15px'}),
        ui.Label('Berikut merupakan tabel luas sebaran kekeringan pertanian selama tahun 2013-2022 di Kabupaten Mojokerto yang diiperoleh melalui analisis data citra Landsat 8 Collection 2 Tier 1 TOA Reflectance menggunakan metode Temperature-Vegetation Dryness Index (TVDI)', {fontSize:'12px', margin: '2px 15px 2px 15px', textAlign: 'justify'}),
        ui.Label('[Klik Disini] Tabel Luas Sebaran Kekeringan Pertanian', {fontSize:'12px', margin: '2px 15px 2px 15px', textAlign: 'justify'},'https://raw.githubusercontent.com/nurilhidayati/analisiskekeringanpertanian/main/Tabel%20Luas%20Sebaran%20Kekeringan%20Pertanian%20di%20Kabupaten%20Mojokerto%20Tahun%202013-2022.png'),
        ui.Label(''),
    ]
);
tabelPanel.add(tabelLuas);




